<idea-plugin>
    <id>com.github.login5065.temp</id>
    <name>temp</name>
    <vendor>login5065</vendor>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <applicationService serviceImplementation="com.github.login5065.temp.services.MyApplicationService"/>
        <projectService serviceImplementation="com.github.login5065.temp.services.MyProjectService"/>
    </extensions>

    <applicationListeners>
        <listener class="com.github.login5065.temp.listeners.MyProjectManagerListener"
                  topic="com.intellij.openapi.project.ProjectManagerListener"/>
    </applicationListeners>
</idea-plugin>
